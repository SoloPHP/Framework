{
  "name": "solophp/framework",
  "type": "project",
  "description": "SoloPHP is a simple PSR compatible PHP framework for developing web applications and APIs.",
  "keywords": [
    "SoloPHP",
    "framework",
    "PHP",
    "API",
    "PSR"
  ],
  "homepage": "https://github.com/SoloPHP/Framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Vitaliy Olos",
      "email": "vedastudio@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Solo\\": "Solo/"
    }
  },
  "require": {
    "php": ">=8.1",
    "solophp/application": "^1.0",
    "solophp/container": "^1.0",
    "solophp/configs": "^1.0",
    "solophp/settings": "^1.0",
    "solophp/database": "^2.0",
    "solophp/repository": "^1.2",
    "solophp/storage": "^1.0",
    "solophp/http-client": "^2.0",
    "solophp/http-emitter": "^1.0",
    "solophp/inertia": "^1.1",
    "solophp/flash-messages": "^1.0",
    "solophp/validator": "^1.0",
    "solophp/paginator": "^1.0",
    "solophp/translate": "^1.0",
    "solophp/session": "^1.0",
    "solophp/formatter": "^1.0",
    "solophp/logger": "^1.0",
    "solophp/image-uploader": "^1.0",
